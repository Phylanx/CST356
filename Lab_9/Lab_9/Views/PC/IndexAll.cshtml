@model IEnumerable<Lab_9.Models.PCViewModel>

@{
    ViewBag.Title = "Index-All";
}

<h2>IndexAll</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GB_Memory)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GB_Storage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MHZ_Processor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReadyForUpgrade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GB_Memory)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GB_Storage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MHZ_Processor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReadyForUpgrade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
    </tr>
}

</table>
